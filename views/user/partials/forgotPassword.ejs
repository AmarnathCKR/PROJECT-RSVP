<%- include('../layouts/header') %>

    <div class="row g-0 my-3">

        <div class="col-lg-12 d-flex justify-content-center g-0 my-3">
            <p class="text-danger">
                <%= locals.error%>
            </p>
        </div>

        <div class="col-lg-12 d-flex justify-content-center g-0">
            <form action="" method="post" class="justify-content-center">
                <div class="col-lg-12 d-flex justify-content-center g-0 my-3">
                    <h5>Enter your registered email</h5>

                </div>
                <div class="mb-3">


                    <label for="email" class="form-label">Email</label>
                    <input class="form-control" id="email" name="email" />

                </div>

                <div class="mb-3 d-flex justify-content-center">
                    <button type="submit" class="btn btn-dark m-1">Verify email</button><br />
                </div>
            </form>

        </div>

    </div>


<%- include('../layouts/footer') %>